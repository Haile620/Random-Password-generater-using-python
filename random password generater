# mini random password generator having 10 charactes 
from random import randint

# All uppercase leters
password = ""
for i in range(10):
    i = chr(randint(65, 90))
    password = str(password) + i
print(password)

# All uppercase leters
password = ""
for i in range(10):
    i = chr(randint(65, 90)).lower()
    password = str(password) + i
print(password)

# Combination of  uppercase  and lowercase leters
password = ""
for i in range(5):
    i = chr(randint(65, 90))
    for j in range(5):
        j = chr(randint(65, 90)).lower()
    password = str(password) + i + j
print(password)
